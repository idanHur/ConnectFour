@page
@model IEnumerable<dynamic>

<h2>Players Sorted by Name (Descending)</h2>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Last Game Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var player in Model)
        {
            <tr>
                <td>@player.playerName</td>
                <td>@player.LastGameDate?.ToString("yyyy-MM-dd")</td>
            </tr>
        }
    </tbody>
</table>

@{
}
